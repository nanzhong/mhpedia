<h1>Armor Skills</h1>
<div class="row">
  <div class="small-12 columns">
    <ul class="small-block-grid-1 large-block-grid-2">
      <% @skills.each do |skill| %>
        <li>
        <div class="panel">
          <%= link_to skill.name, skill %>
        </div>
        </li>
      <% end %>
    </ul>
  </div>
</div>
